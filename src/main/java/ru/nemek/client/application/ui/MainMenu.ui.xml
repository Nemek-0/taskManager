<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <ui:with type="ru.nemek.client.place.NameTokens" field="nameTokens"></ui:with>

    <g:HTMLPanel tag="nav">
        <b:Navbar>
            <b:NavbarCollapse >
                <b:NavbarNav>
                    <b:AnchorListItem ui:field="anchorListItemHome" href="#{nameTokens.getHOME}">Home</b:AnchorListItem>
                    <b:AnchorListItem ui:field="anchorListItemHistory" href="#{nameTokens.getHistory}">History</b:AnchorListItem>
                </b:NavbarNav>
            </b:NavbarCollapse>
        </b:Navbar>
    </g:HTMLPanel>
</ui:UiBinder>
